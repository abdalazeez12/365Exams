@model ContentPageViewModel
@await Html.PartialAsync("_Breadcrumbs", Model)

<div class="about-area default-padding">
    <div class="container">
        <div class="row">
            <div class="about-items">
                @for (int i = 0; i < Model.StaticPage.Contents.Count; i++)
                {
                    <h2>@Model.StaticPage.Contents[i].LocalizedName</h2>
                    @Html.Raw(Model.StaticPage.Contents[i].LocalizedDescription)
                }
            </div>
        </div>
    </div>
</div>